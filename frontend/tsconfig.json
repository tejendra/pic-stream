{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react-jsx",
    "target": "ESNext",
    "types": ["vite/client"],
    "isolatedModules": true,
    "allowImportingTsExtensions": true,
    "module": "Node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true
  }
}
